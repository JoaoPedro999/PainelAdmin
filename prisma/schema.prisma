generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgrelsql"
  url      = env("DATABASE_URL")
}

model Project_Primary_Registers {
id String @id @default(uuid())
CNPJ String @unique
Gestor String
Empresa String @unique

@@map("Cadastros")
}



